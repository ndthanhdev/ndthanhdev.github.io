{
	"extends": "../tsconfig.options.json",
	"compilerOptions": {
		"jsx": "react-jsx",
		"jsxImportSource": "@emotion/react",
		"baseUrl": ".",
		"paths": {
			"@/*": ["src/*"]
		},
		"outDir": "../.moon/cache/types/app"
	},
	"include": [
		"./src/**/*.ts",
		"./src/**/*.tsx",
		"./content/**/*",
		"gatsby-node.ts",
		"gatsby-config.ts"
	]
}
